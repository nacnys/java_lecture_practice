package object;

class Book{
	String title;
	String author;
	
	Book(String title, String author){
		this.title = title;
		this.author = author;
	}
	
	
}


public class ToStringEx {

	public static void main(String[] args) {

		Book book = new Book("두잇자바", "은종님");
		System.out.println(book);
		
	}

}
